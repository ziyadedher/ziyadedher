{
  "name": "ziyadedher",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "build": "npm run storage:download-bundle && next build",
    "start": "next start",
    "storage:setup": "gcloud auth login && gcloud config set project ziyadedher && gcloud auth application-default login",
    "storage:download": "gcloud storage rsync --recursive gs://ziyadedher ./public",
    "storage:upload": "gcloud storage rsync --recursive ./public gs://ziyadedher",
    "storage:bundle": "npm run storage:download && rm -f ./public/assets.tar.gz && tar -zcvf ./public/assets.tar.gz ./public && npm run storage:upload",
    "storage:download-bundle": "curl https://storage.googleapis.com/ziyadedher/assets.tar.gz --output assets.tar.gz && tar -zxvf assets.tar.gz && rm -f assets.tar.gz"
  },
  "dependencies": {
    "@headlessui/react": "2.2.4",
    "@heroicons/react": "2.2.0",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "chart.js": "4.4.9",
    "classnames": "2.5.1",
    "clsx": "2.1.1",
    "flowbite": "2.5.2",
    "flowbite-react": "0.11.7",
    "framer-motion": "11.18.2",
    "next": "15.2.5",
    "onnxruntime-web": "1.22.0",
    "primereact": "10.9.6",
    "prism-react-renderer": "2.4.1",
    "random-js": "2.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "5.5.0",
    "sharp": "0.34.2",
    "swr": "2.3.3",
    "zod": "3.25.48"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.2.5",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@typescript-eslint/parser": "8.33.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.28.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "postcss": "8.5.4",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.12",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1"
  }
}
