{
  "name": "ziyadedher",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint": "next lint",
    "build": "npm run storage:download-bundle && next build",
    "start": "next start",
    "storage:setup": "gcloud auth login && gcloud config set project ziyadedher && gcloud auth application-default login",
    "storage:download": "gcloud storage rsync --recursive gs://ziyadedher ./public",
    "storage:upload": "gcloud storage rsync --recursive ./public gs://ziyadedher",
    "storage:bundle": "npm run storage:download && rm -f ./public/assets.tar.gz && tar -zcvf ./public/assets.tar.gz ./public && npm run storage:upload",
    "storage:download-bundle": "curl https://storage.googleapis.com/ziyadedher/assets.tar.gz --output assets.tar.gz && tar -zxvf assets.tar.gz && rm -f assets.tar.gz"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.1",
    "chart.js": "^4.4.1",
    "classnames": "^2.3.2",
    "flowbite": "^2.0.0",
    "flowbite-react": "^0.10.0",
    "next": "14.2.21",
    "onnxruntime-web": "^1.16.3",
    "primereact": "^10.2.1",
    "random-js": "^2.1.0",
    "react": "^18",
    "react-color": "^2.19.3",
    "react-console-emulator": "^5.0.2",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "sharp": "^0.33.0",
    "swr": "^2.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-color": "^3.0.9",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^7.5.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "14.2.21",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}